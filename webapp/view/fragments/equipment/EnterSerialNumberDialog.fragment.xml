<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">

	<Dialog horizontalScrolling="false" icon="sap-icon://edit" showHeader="true" state="None" stretch="false"
		title="{i18n>equi.serial.dlg.title}" type="Standard" verticalScrolling="false">
		<content>
			<Input placeholder="{i18n>equi.serial.number}" value="{equipmentDetailsModel>/data/EquipmentId}" type="Text" showValueHelp="false"   width="100%" valueHelpOnly="false" maxLength="0"/>
		</content>
		<beginButton/>
		<buttons>
			<Button text="{i18n>label.ok}" type="Default" iconFirst="true" width="auto"   iconDensityAware="false" press="handleEnterSerialConfirm"/>
			<Button text="{i18n>label.cancel}" type="Default" iconFirst="true" width="auto"   iconDensityAware="false" press="handleEnterSerialDecline"/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>

</core:FragmentDefinition>
