<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core">
    <Dialog
            title="{okCancelDialogDataModel>/title}"
            icon="sap-icon://question-mark"
    >
        <content>
            <Text text="{okCancelDialogDataModel>/text}" class="sapUiSmallMargin"/>
        </content>
        <beginButton>
            <Button
                    text="{i18n>label.ok}"
                    icon="sap-icon://accept">
            </Button>
        </beginButton>
        <endButton>
            <Button
                    text="{i18n>label.cancel}"
                    icon="sap-icon://cancel">
            </Button>
        </endButton>
    </Dialog>
</core:FragmentDefinition>
